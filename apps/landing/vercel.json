{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "npm install --prefix ../.. && npm run build --workspace=packages/ui --workspace=packages/shared --workspace=packages/chain --if-present",
  "buildCommand": "cd ../.. && npx turbo run build --filter=@proofchain/landing",
  "outputDirectory": ".next",
  "ignoreCommand": "npx turbo-ignore"
}
